{
  "name": "react-native-tapjoy",
  "description": "A React Native module that allows you to use the native Tapjoy advertiser SDK.",
  "version": "2.4.3",
  "license": "MIT",
  "main": "src/index.js",
  "author": "Marius Reimer (reime005) <reime005@gmail.com> (https://mariusreimer.com)",
  "types": "./src/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/reime005/react-native-tapjoy.git"
  },
  "homepage": "https://github.com/reime005/react-native-tapjoy#readme",
  "keywords": [
    "react",
    "react native",
    "react hooks",
    "react native tapjoy",
    "ads",
    "typescript"
  ],
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "eslint --fix",
      "git add"
    ],
    "*.ts*": [
      "yarn prettier",
      "tslint --fix",
      "git add"
    ],
    "*.d.ts": [
      "yarn prettier",
      "git add"
    ]
  },
  "scripts": {
    "clean": "rimraf ./android/build/ && rimraf ./ios/build/",
    "precommit": "lint-staged && npm run test",
    "test": "jest --passWithNoTests",
    "test:coverage": "npm run test -- --coverage",
    "test:coverage:deploy": "npm run test:coverage && codecov",
    "release": "semantic-release"
  },
  "resolutions": {
    "@types/react": "^16.8.6",
    "react-native": "^0.60.0"
  },
  "peerDependencies": {
    "react-native": ">=0.60.0",
    "react": ">=16.8.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.4.0",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@commitlint/prompt": "^8.2.0",
    "@semantic-release/commit-analyzer": "^6.3.3",
    "@semantic-release/git": "^7.0.18",
    "@semantic-release/github": "^5.5.5",
    "@semantic-release/npm": "^5.3.4",
    "@semantic-release/release-notes-generator": "^7.3.5",
    "@types/jest": "^24.0.11",
    "@types/react": "16.8.3",
    "@types/react-native": "^0.57.47",
    "app-root-dir": "^1.0.2",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "change-case": "^3.1.0",
    "codecov": "^3.3.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-native-prettier": "^1.0.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-react-native": "^3.7.0",
    "husky": "^1.3.1",
    "immer": "^2.1.5",
    "jest": "^24.7.1",
    "jest-dom": "^3.1.3",
    "lint-staged": "^8.1.5",
    "metro-react-native-babel-preset": "^0.53.1",
    "prettier": "^1.17.0",
    "prettier-eslint": "^8.8.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-native": "^0.60.0",
    "react-testing-library": "^6.1.2",
    "rimraf": "^2.6.3",
    "semantic-release": "^17.2.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslib": "^1.9.3",
    "tslint": "^5.16.0",
    "typescript": "^3.4.4",
    "typings-tester": "^0.3.2"
  },
  "jest": {
    "preset": "react-native",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.js?$": "babel-jest"
    },
    "transformIgnorePatterns": [],
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "functions": 50,
        "lines": 70,
        "statements": 70
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/node_modules/regenerator-runtime/runtime",
      "react-testing-library/cleanup-after-each",
      "jest-dom/extend-expect"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(coverage|dist|node_modules|tools)/"
    ],
    "verbose": true
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "root": true,
    "env": {
      "browser": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "extends": [
      "airbnb",
      "prettier"
    ],
    "rules": {
      "array-callback-return": 0,
      "camelcase": 0,
      "import/no-extraneous-dependencies": 0,
      "import/prefer-default-export": 0,
      "no-nested-ternary": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-filename-extension": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/no-array-index-key": 0,
      "react/react-in-jsx-scope": 0,
      "react/sort-comp": 0
    }
  },
  "eslintIgnoreConfig": [
    "node_modules/",
    "coverage/",
    "dist/",
    "*.ts"
  ],
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/release-notes-generator"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "parser": "typescript"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  }
}
